{
  "name": "@Chicken/shared-client",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "rimraf ./build && tsc && npm run copy-scss",
    "watch": "concurrently npm:watch-ts npm:watch-scss",
    "watch-ts": "rimraf ./build && tsc -w --preserveWatchOutput",
    "watch-scss": "nodemon --watch \"./src\" -e scss --exec \"npm run copy-scss\"",
    "copy-scss": "copyfiles -u 1 \"./src/**/*.scss\" ./build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "events": "^3.3.0",
    "file-saver": "^2.0.5",
    "howler": "^2.2.4",
    "rc-slider": "^11.1.6",
    "react": "^18.3.1",
    "react-datepicker": "^6.3.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.0.5",
    "react-intersection-observer": "^9.8.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "react-textarea-autosize": "^8.5.3",
    "react-tooltip": "^5.27.0",
    "usehooks-ts": "^3.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/howler": "^2.2.11",
    "@types/node": "^18.19.18",
    "@types/react": "^18.2.60",
    "@types/react-datepicker": "^6.0.1",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-textarea-autosize": "^8.0.0",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.6",
    "typescript": "^5.4.5"
  }
}
