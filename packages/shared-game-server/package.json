{
  "name": "@Chicken/shared-game-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev-start": "rollup -c -w --bundleConfigAsCjs",
    "start": "node ./build/index.js",
    "test": "vitest --no-file-parallelism",
    "test-coverage": "vitest --no-file-parallelism --coverage",
    "deploy-staging": "npm run build && eb deploy chicken-game-staging",
    "deploy-production": "npm run build && eb deploy chicken-game-production"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.592.0",
    "@slack/web-api": "^7.2.0",
    "@testcontainers/mongodb": "^11.0.3",
    "axios": "^1.6.7",
    "customerio-node": "^4.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "fireblocks-sdk": "^5.26.0",
    "mongodb": "^6.3.0",
    "nanoid": "^5.0.6",
    "rate-limiter-flexible": "^4.0.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-run": "^3.0.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^18.19.18",
    "@types/web-push": "^3.6.3",
    "@vitest/coverage-v8": "^3.0.8",
    "rollup": "^4.21.2",
    "rollup-plugin-delete": "^2.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vitest": "^3.0.8"
  }
}
